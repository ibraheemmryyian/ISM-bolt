{
  "version": 2,
  "name": "symbioflows",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.symbioflows.com/api/$1"
    },
    {
      "src": "/ai/(.*)",
      "dest": "https://api.symbioflows.com/ai/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://api.symbioflows.com",
    "VITE_AI_SERVICES_URL": "https://api.symbioflows.com/ai",
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key"
  },
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  }
}
