# SymbioFlows Production Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# SUPABASE CONFIGURATION
# =============================================================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key
SUPABASE_DB_PASSWORD=symbioflows123

# =============================================================================
# AI API KEYS
# =============================================================================
DEEPSEEK_API_KEY=your-deepseek-api-key
OPENAI_API_KEY=your-openai-api-key

# =============================================================================
# JWT & SECURITY
# =============================================================================
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
CORS_ORIGIN=http://localhost:5173

# =============================================================================
# FRONTEND CONFIGURATION
# =============================================================================
VITE_API_URL=http://localhost:3000
VITE_AI_SERVICES_URL=http://localhost:5000
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
DATABASE_URL=postgresql://postgres:symbioflows123@localhost:5432/symbioflows

# =============================================================================
# REDIS CONFIGURATION
# =============================================================================
REDIS_URL=redis://localhost:6379

# =============================================================================
# MONITORING CONFIGURATION
# =============================================================================
GRAFANA_PASSWORD=admin
PROMETHEUS_RETENTION_TIME=200h

# =============================================================================
# AI SERVICES CONFIGURATION
# =============================================================================
LOG_LEVEL=INFO
MAX_WORKERS=4
REQUEST_TIMEOUT=60
FLASK_ENV=production

# =============================================================================
# PRODUCTION SETTINGS
# =============================================================================
NODE_ENV=production
PORT=3000
FLASK_APP=ai_gateway.py

# =============================================================================
# OPTIONAL: STRIPE PAYMENT PROCESSING
# =============================================================================
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# =============================================================================
# OPTIONAL: EMAIL CONFIGURATION
# =============================================================================
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# =============================================================================
# OPTIONAL: LOGGING CONFIGURATION
# =============================================================================
LOG_FILE=logs/symbioflows.log
LOG_LEVEL_FILE=INFO
LOG_LEVEL_CONSOLE=INFO

# =============================================================================
# OPTIONAL: CACHE CONFIGURATION
# =============================================================================
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# =============================================================================
# OPTIONAL: RATE LIMITING
# =============================================================================
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX_REQUESTS=100

# =============================================================================
# OPTIONAL: FILE UPLOAD CONFIGURATION
# =============================================================================
MAX_FILE_SIZE=10485760
UPLOAD_PATH=uploads
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# =============================================================================
# OPTIONAL: ANALYTICS CONFIGURATION
# =============================================================================
GOOGLE_ANALYTICS_ID=your-ga-id
MIXPANEL_TOKEN=your-mixpanel-token

# =============================================================================
# OPTIONAL: EXTERNAL API INTEGRATIONS
# =============================================================================
FREIGHTOS_API_KEY=your-freightos-api-key
SHIPPING_API_KEY=your-shipping-api-key
CARBON_API_KEY=your-carbon-api-key 